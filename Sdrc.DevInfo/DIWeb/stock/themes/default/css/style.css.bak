	display: block;
	text-decoration: none;
}
.sc_menu span{
	display: none;
	margin-top: 3px;
	text-align: center;
	font-size: 12px;	
	color: #fff;
}
.sc_menu a:hover span{
	display: block;
}
.sc_menu img{
	border: 3px #fff solid;	
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
}
.sc_menu a:hover img{
	filter:alpha(opacity=50);	
	opacity: 0.5;
}

